{
    "name": "davidsneal/netlify",
    "description": "A basic Netlify addon for Statamic v3.",
    "keywords": [
        "Statamic",
        "Addon",
        "Netlify"
    ],
    "type": "statamic-addon",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Davidsneal\\Netlify\\": "src"
        }
    },
    "authors": [
        {
        "name": "David Neal"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "6.*",
        "kitetail/zttp": "^0.6.0",
        "statamic/cms": "3.*@beta"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Davidsneal\\Netlify\\ServiceProvider"
            ]
        },
        "statamic": {
            "name": "Netlify",
            "description": "View latest deploys and trigger a new deployment in Netlify"
        }
    }
}
